{
	"apiVersion": 3,
	"name": "smart-send/pickup-point-block",
	"version": "2.0.0",
	"title": "Smart Send logistics Pickup Points",
	"category": "woocommerce",
	"description": "Show pick-up point selector during checkout for relevant shipping methods",
	"supports": {
		"html": false,
		"align": false,
		"multiple": false,
		"reusable": false
	},
	"parent": [ "woocommerce/checkout-shipping-methods-block" ],
	"attributes": {
		"lock": {
			"type": "object",
			"default": {
				"remove": true,
				"move": true
			}
		},
		"text": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "smart-send-logistics",
	"editorStyle": "file:../../../build/style-smart-send-block.css"
}
